package OOP;

public class Program {

    public static void main(String[] args) {
        Calculator calculator = new Calculator();
        System.out.println(calculator.countDigits(1341));
        System.out.println(calculator.evenOdd(1341));
        System.out.println(calculator.getLastDigit(1341));
        System.out.println(calculator.getDigit(1341, 1));
        System.out.println(calculator.setDigit(1341, 1, 6));
    }
}
