import java.util.Scanner;

public class Lesson10 {
    static Scanner scan = new Scanner(System.in);

    public static void main(String[] args) {
        int n= scan.nextInt();
        for (int i = 2; i <n ; i++) {
            if(n % i == 0){
                System.out.println("not prime number!!");
                break;
            }
            else if(i == n - 1){
                System.out.println("prime number!!");
                break;
            }
        }
    }
}
